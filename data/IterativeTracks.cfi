module iterativeGSMerger = TrackMerger {
    VInputTag src = { 
	firstGSVertexFilter1,
	firstGSVertexFilter2,
	secondGSVertexFilter1,
	secondGSVertexFilter2,
	thirdGSVertexFilter1,
	thirdGSVertexFilter2,
	fourthGSVertexFilter1,
	fourthGSVertexFilter2
    }
}

module iterativeGSWithMaterialTracks = AnyTrackFullCloneSelector { 
    InputTag src = iterativeGSMerger
}
